{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "predis/predis": "~1.1",
        "app/ufa": "dev-master",
        "app/foundation": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.3",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "core/database"
        ],
        "psr-4": {
            "App\\Admin\\": "app-admin/app/",
            "App\\Web\\": "app-web/app/",
            "App\\": "core/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Admin\\": "app-admin/tests/",
            "Tests\\Web\\": "app-web/tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php app-admin/artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php app-admin/artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php app-admin/artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        },
        "ufa": {
            "type": "git",
            "url": "git@106.14.170.179:service/ufa.git"
        },
        "foundation": {
            "type": "git",
            "url": "git@106.14.170.179:service/foundation.git"
        }
    }
}
